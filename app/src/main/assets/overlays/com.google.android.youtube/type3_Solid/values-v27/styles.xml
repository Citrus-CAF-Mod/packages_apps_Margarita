<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Base.V27.Theme.YouTube.Light.Dialog" parent="@*com.google.android.youtube:style/Base.V23.Theme.YouTube.Light.Dialog">
         <item name="android:navigationBarColor">@*android:color/holo_blue_light</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>

    <style name="Base.V27.Theme.YouTube.Share.DialogFragment.Dark" parent="@*com.google.android.youtube:style/Base.Theme.YouTube.Share.DialogFragment.Dark">
        <item name="android:navigationBarColor">@*android:color/holo_blue_light</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@android:color/transparent</item>
    </style>

    <style name="Base.V27.Theme.YouTube.Share.DialogFragment.Light" parent="@*com.google.android.youtube:style/Base.Theme.YouTube.Share.DialogFragment.Dark">
        <item name="android:navigationBarColor">@*android:color/holo_blue_light</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/transparent</item>
    </style>

    <style name="NavigationBar.Dark.V27" parent="@*com.google.android.youtube:style/NavigationBar">
        <item name="*com.google.android.youtube:navigationBarColor">@*android:color/holo_blue_light</item>
        <item name="*com.google.android.youtube:navigationBarDividerColor">@null</item>
        <item name="*com.google.android.youtube:navigationBarDividerSize">0.0dip</item>
    </style>

    <style name="NavigationBar.Light.V27" parent="@*com.google.android.youtube:style/NavigationBar">
        <item name="*com.google.android.youtube:navigationBarColor">@*android:color/holo_blue_light</item>
        <item name="*com.google.android.youtube:navigationBarControlsMode">dark</item>
        <item name="*com.google.android.youtube:navigationBarDividerColor">@null</item>
        <item name="*com.google.android.youtube:navigationBarDividerSize">0.0dip</item>
    </style>

    <style name="Theme.YouTube.Home" parent="@*com.google.android.youtube:style/Base.V27.Theme.YouTube.Home">
        <item name="android:navigationBarColor">@*android:color/holo_blue_light</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>

</resources>
