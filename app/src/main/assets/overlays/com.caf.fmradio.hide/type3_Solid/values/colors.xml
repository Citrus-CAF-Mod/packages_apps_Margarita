<resources>

    <!-- Favorite bottom bar-->
    <color name="favorite_button_normal">@*android:color/holo_blue_dark</color> <!-- color of bottom bar -->
    <color name="favorite_button_active">@*android:color/holo_blue_light</color> <!-- color of bottom bar when pressed -->

    <!-- frequency area -->
    <color name="picker_background_color">@*android:color/holo_blue_light</color> <!-- background of frequency dial and spikes -->

    <!-- Frequency dialog -->
    <color name="picker_frequency_dialog_khz_background_color">@*android:color/holo_blue_light</color> <!-- background color of kHz on frequency dialog -->
    <color name="picker_frequency_dialog_mhz_background_color">@*android:color/holo_blue_light</color> <!-- background color of mHz on frequency dialog -->

    <!-- Material -->
    <color name="primary">@*android:color/holo_blue_light</color>
    <color name="primary_dark">@*android:color/holo_blue_dark</color>
    <color name="background_color">@*android:color/background_dark</color>

</resources>
